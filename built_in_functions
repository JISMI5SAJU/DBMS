select * from dual;
select ceil(36.29) from dual;
select ceil(-36.9) from dual;
select ceil(36/3) from dual;
select FLOOR(36.76) from dual;
select FLOOR(36/4) from dual;
select sqrt(625) from dual;
select sqrt(36) from dual;
select abs(36.29) from dual;
select abs(-36.14) from dual;
select abs(-36) from dual;
SELECT SYSDATE AS NOW FROM DUAL;
SELECT SYSTIMESTAMP FROM DUAL;
select least ('apple','banana','cherry') from dual;
select greatest ('apple','banana','cherry') from dual;
select ltrim('Trim me') from dual;
select rtrim('I LOVE INDIA      ')FROM DUAL;
select to_char(sysdate,'YYYY_MM_DD HH24:MI:SS') as Format1,to_char(sysdate,'DD_MM_YY HH12:MI:SS') AS Format2 from dual;
select lpad('123',6,'0') from dual;
SELECT reverse('uoyevol I') FROM DUAL;
SELECT LENGTH('Oracle Database') from dual;
SELECT 'Oracle'||''||' SQL'||''||' is'||''||' Powerful'||''||' flexible' from dual;
select concat('Jismi','Saju') from dual;
select substr('India is my country',7,2) from dual;
SELECT SUBSTR('JISMI',2,3) FROM DUAL;
SELECT SUBSTR('Substring',1,3) from dual;
SELECT REVERSE('racecar') from dual;
select rpad('jismi',15,'X') from dual;
